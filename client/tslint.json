{
	"defaultSeverity": "error",
	"extends": ["tslint:recommended"],
	"jsRules": {},
	"rules": {
		"quotemark": [true, "single"],
		"semicolon": [true, "never"],
		"indent": [true, "tabs", 4],
		"arrow-parens": false,
		"object-literal-sort-keys": [true, "match-declaration-order"],
		"ordered-imports": [false]
	}
}
